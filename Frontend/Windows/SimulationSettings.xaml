                        <Setter.Value>
                            <ImageBrush ImageSource="/Frontend/gui/Buttons/CancelButtonHover.png"/>
                        </Setter.Value>
                    </Setter>
                </Trigger>
            </ControlTemplate.Triggers>
        </ControlTemplate>

